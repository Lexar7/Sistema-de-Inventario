                <SelectCommand>
                  <DbCommand CommandType="Text" ModifiedByUser="true">
                    <CommandText>SELECT        cliente.Nombre AS Nombre_Cliente, cliente.Apellido AS Apellido_Cliente, cliente.Direccion, usuarios.Nombre AS Empleado, producto.Nombre AS Producto, 
                         categoria.Nombre AS Categoria, detalle_venta.Precio_Unitario, detalle_venta.Cantidad, venta.Fecha_Venta, 
                         SUM(detalle_venta.Cantidad * detalle_venta.Precio_Unitario) AS Total_Producto
FROM            categoria INNER JOIN
                         producto ON categoria.IdCategoria = producto.IdCategoria INNER JOIN
                         detalle_venta ON producto.IdProducto = detalle_venta.IdProducto INNER JOIN
                         venta ON detalle_venta.IdVenta = venta.IdVenta INNER JOIN
                         cliente ON venta.IdCliente = cliente.IdCliente INNER JOIN
                         usuarios ON venta.IdUsuario = usuarios.IdUsuario
WHERE producto.Nombre=? 
GROUP BY cliente.Nombre, cliente.Apellido, cliente.Direccion  ,usuarios.Nombre, producto.Nombre, categoria.Nombre, detalle_venta.Precio_Unitario, detalle_venta.Cantidad, venta.Fecha_Venta, 
                         cliente.Direccion
ORDER BY Nombre_Cliente, Producto</CommandText>
